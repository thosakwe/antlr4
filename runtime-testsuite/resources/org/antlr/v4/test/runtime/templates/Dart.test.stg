TokenGetterListener(X) ::= <<
@parser::members {
 class LeafListener extends TBaseListener {
	 void exitA(TParser.AContext ctx) {
		if (ctx.childCount==2)
			print("${ctx.INT(0).symbol.text} ${ctx.INT(1).symbol.text} ${ctx.INT()}");
		else
			print(ctx.ID().symbol);
	}
}
}
>>

RuleGetterListener(X) ::= <<
@parser::members {
 class LeafListener extends TBaseListener {
	 void exitA(TParser.AContext ctx) {
		if (ctx.childCount==2) {
			print("${ctx.b(0).start.text} ${ctx.b(1).start.text} ${ctx.b().get(0).start.text}");
		} else
			print(ctx.b(0).start.text);
	}
}
}
>>


